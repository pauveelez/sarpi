
#btn_feed
  background-color rgb(87, 182, 219)
  border-radius 100%
  box-shadow 0px 0px 5px 20px rgb(159, 211, 240)
  cursor pointer
  margin auto
  min-height 200px
  max-width 200px
  min-width 100px
  p
    color rgb(29, 117, 249)
    font-weight bold
    font-size 2em
    margin-top 30px
    padding-top 40%



#chart_report
  background-color #bcdbeb
  margin 0
  width 55%
  height 410px
  text-align center

#reports
  fieldset
    margin 0
    padding 0
    border none
    padding-bottom 1em

#type_report
  margin 0
  padding 0
  input[type="radio"]
    margin 8px 0 0 10px
  label
    color red


.special
  border-radius 10px
  cursor pointer
  margin 10px
  width 10px
  padding 8px 10px 0
  -o-user-select none
  user-select none



.circular
  border-radius 100%
  height 120px
  margin 20px
  width 120px

.small_circular
  border-radius 100%
  height 45px
  width 45px





.menu
  margin 0
  text-align center
  position relative
  width 100%
  height 100%
  overflow hidden
  padding-top 6px
  a
    text-decoration none

.right
  float right
  padding-right 12px

.left
  img
    float left
    padding-left 12px
    width 50px


