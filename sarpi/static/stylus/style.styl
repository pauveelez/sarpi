
#btn_feed
  background-color rgb(87, 182, 219)
  border-radius 100%
  box-shadow 0px 0px 5px 20px rgb(159, 211, 240)
  cursor pointer
  margin auto
  min-height 200px
  max-width 200px
  min-width 100px
  p
    color rgb(29, 117, 249)
    font-weight bold
    font-size 2em
    margin-top 30px
    padding-top 40%

#chart_report
  background-color #bcdbeb
  margin 0
  width 100%
  height 350px
  text-align center

#type_report
  margin 0
  padding 0
  list-style none
  input[type="radio"]
    margin 8px 0 0 10px